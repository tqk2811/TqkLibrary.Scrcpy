		<PackageReference Include="TqkLibrary.WpfUi" Version="1.0.0-build2022050602" />
		<PackageReference Include="TqkLibrary.AutoGrid" Version="1.2.29" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\TqkLibrary.Scrcpy.Wpf\TqkLibrary.Scrcpy.Wpf.csproj" />
