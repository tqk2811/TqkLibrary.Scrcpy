		<file src="..\x64\Release\TqkLibrary.ScrcpyNative.dll" target="runtimes\win-x64\native\" />
		<file src="..\x86\Release\TqkLibrary.ScrcpyNative.dll" target="runtimes\win-x86\native\" />

		<file src="..\scrcpy-server-v2.0.jar" target="contentFiles\any\any\scrcpy-server.jar" />
