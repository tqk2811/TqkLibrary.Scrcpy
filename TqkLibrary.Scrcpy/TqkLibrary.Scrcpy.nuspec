<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<!--https://docs.microsoft.com/en-us/nuget/reference/nuspec-->
	<metadata>
		<id>$id$</id>
		<version>1.0.0</version>
		<description>my private lib</description>
		<authors>tqk2811</authors>
		<projectUrl>https://github.com/tqk2811/TqkLibrary.Scrcpy</projectUrl>
		<repository type="git" url="https://github.com/tqk2811/TqkLibrary.Scrcpy.git"/>
		<license type="expression">MIT</license>
		<dependencies>
			<group targetFramework="net462">
				<dependency id="System.Drawing.Common" version="6.0.0" />
			</group>
			<group targetFramework="net5.0-windows7.0">
				<dependency id="System.Drawing.Common" version="6.0.0" />
			</group>
			<group targetFramework="net6.0-windows7.0">
				<dependency id="System.Drawing.Common" version="6.0.0" />
			</group>
		</dependencies>
		<contentFiles>
			<files include="x64/*.dll" copyToOutput="true" buildAction="None" flatten="true"/>
			<files include="scrcpy-server1.19.jar" copyToOutput="true" buildAction="None" flatten="true"/>
		</contentFiles>
	</metadata>

	<files>
		<file src="bin\Release\**\$id$.dll" target="lib\" />
		<file src="bin\Release\**\$id$.pdb" target="lib\" />
		<file src="bin\Release\**\$id$.xml" target="lib\" />
		<file src="bin\Release\net462\x64\*.dll" target="contentFiles\any\any\x64\" />
		<file src="bin\Release\net462\scrcpy-server1.19.jar" target="contentFiles\any\any\" />
	</files>
</package>